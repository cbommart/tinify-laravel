{
    "name": "jargoud/tinify-laravel",
    "type": "library",
    "description": "Tinify API support with Laravel",
    "keywords": [
        "tinify",
        "tinypng",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "yasmuru",
            "email": "yasmuru@gmail.com"
        },
        {
            "name": "Jérémy Argoud",
            "email": "jeremy.argoud@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/bus": "^9.0|^10.0|^11.0",
        "illuminate/queue": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "jargoud/tinify-php": "^2.0"
    },
    "require-dev": {
        "phpro/grumphp": "^1.15",
        "squizlabs/php_codesniffer": "^3.7.2"
    },
    "autoload": {
        "psr-4": {
            "Jargoud\\LaravelTinify\\": "src"
        }
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:jargoud/tinify-php.git"
        }
    ],
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
